{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ui-appcenter": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "src/tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "assets": [
              "src/assets",
              "src/config.js",
              "src/favicon.ico",
              "src/data",
              {
                "glob": "**/*",
                "input": "node_modules/@covalent/code-editor/assets/monaco",
                "output": "/assets/monaco"
              }
            ],
            "styles": [
              "src/theme.scss",
              "src/styles.scss"
            ],
            "scripts": [
              "node_modules/hammerjs/hammer.min.js",
              "node_modules/showdown/dist/showdown.js",
              "node_modules/intl/dist/Intl.min.js",
              "node_modules/intl/locale-data/jsonp/en-US.js"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ui-appcenter:build",
            "proxyConfig": "e2e/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "ui-appcenter:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ui-appcenter:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "scripts": [
              "node_modules/hammerjs/hammer.min.js",
              "node_modules/showdown/dist/showdown.js",
              "node_modules/intl/dist/Intl.min.js",
              "node_modules/intl/locale-data/jsonp/en-US.js"
            ],
            "styles": [
              "src/theme.scss",
              "src/styles.scss"
            ],
            "assets": [
              "src/assets",
              "src/config.js",
              "src/favicon.ico",
              {
                "glob": "**/*",
                "input": "node_modules/@covalent/code-editor/assets/monaco",
                "output": "/assets/monaco"
              }
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "src/tsconfig.app.json",
              "src/tsconfig.spec.json"
            ],
            "exclude": []
          }
        }
      }
    },
    "ui-appcenter-e2e": {
      "root": "",
      "sourceRoot": "e2e",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "ui-appcenter:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [],
            "exclude": []
          }
        }
      }
    }
  },
  "defaultProject": "ui-appcenter",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "app",
      "styleext": "scss"
    },
    "@schematics/angular:directive": {
      "prefix": "app"
    }
  }
}
